{
  // "$schema": "https://json.schemastore.org/chrome-manifest",
  "manifest_version": 3,
  "name": "textarea history",
  "description": "textarea history",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": {
    "matches": ["<all_urls>"],
    "js": ["main.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  }
}
